#include int main()
{
int num, lbk, sus, aga, ege, lk;

printf("Digite um número: ");
scanf("%d", &num);
printf("Digite outro número: ");
scanf("%d", &lbk);
sus = num + lbk;
aga = num / lbk;
ege = num - lbk;
lk = num * lbk;
printf("A soma é: %d \n", sus);
printf("A divisão é: %d \n", aga);
printf("Subtraindo temos: %d \n", ege);
printf("A multiplicação é: %d \n ", lk);
return 0;
}
